S'function T:onEnter ()\n\tself.bonusCount = 0\n\tself.numBonuses = 2\n\tlevel:paddleWithName ("bonus1"):setEnabled (true)\n\tlevel:paddleWithName ("bonus2"):setEnabled (true)\n\n\tself:enableGravity (true)\n\tlevel:showHUD (true)\n\n\tlocal messages\n\tmessages = {\n\t\t{kMessageTimeout, 1},\n\t\t{mainHero, "Jumps! You better train or you\'ll get stuck!", kEmoticonWinking, 5, "", 5},\n\t\t{mainHero, "The trick is to start the jump sliding up your finger and...", kEmoticonHappy, 12, "", 13},\n\t\t{mainHero, "...keep touching the screen to apply the vertical acceleration.", kEmoticonHappy, 12, "", 13},\n\t\t{mainHero, "For higher jumps, start the slide when I\'m going up!", kEmoticonHappy, 12, "", 0},\n\t}\n\n\tself:startMessagesScript ("trainingFieldJumpMessages", messages)\nend\n\nfunction T:onExit ()\n\tself:enableGravity (false)\n\tlevel:showHUD (false)\n\tself:stopMessagesScript ("trainingFieldJumpMessages")\nend\n\nfunction T:enableGravity (b)\n\tif (b) then\n\t\tmainHero:setSpeed (0, -50)\n\t\tgame.levelSetMinSpeed (0, 0)\n\t\tgame.levelSetMaxSpeed (50, 220)\n\t\tgame.levelSetAccelerationViscosity (0.2, 0.0)\n\t\tgame.levelSetAccelerationFactor (1, 3)\n\t\tgame.levelSetAccelerationMin (-50, -100)\n\t\tgame.levelSetAccelerationMax (50, 150)\n\t\tgame.levelSetGravity (0, -9)\n\t\tgame.levelSetFriction (0.8)\t\n\n\t\tif game.inputMode () == kInputModeJoystick then\n\t\t\tif game.difficultyLevel () == kDifficultyLow then\n\t\t\t\tgame.levelSetAccelerationMin (-100, -100)\n\t\t\t\tgame.levelSetAccelerationMax (100, 170)\n\t\t\t\tgame.levelSetJoystickAccelerationFactor (0.1)\n\t\t\telse\n\t\t\t\tgame.levelSetAccelerationMax (50, 125)\n\t\t\tend\n\t\telse\n\t\t\tif game.difficultyLevel () == kDifficultyLow then\n\t\t\t\tgame.levelSetAccelerationMin (-100, -100)\n\t\t\t\tgame.levelSetAccelerationMax (100, 170)\n\t\t\t\tgame.levelSetJoystickAccelerationFactor (0.1)\n\t\t\tend\n\t\tend\n\telse\n\t\tmainHero:setSpeed (100, 50)\n\t\tgame.levelSetMinSpeed (0, 0)\n\t\tgame.levelSetMaxSpeed (200, 200)\n\t\tgame.levelSetAccelerationViscosity (0.75, 0.75)\n\t\tgame.levelSetAccelerationFactor (1, 1)\n\t\tgame.levelSetAccelerationMin (-100, -100)\n\t\tgame.levelSetAccelerationMax (100, 100)\n\t\tgame.levelSetGravity (0, 0)\n\t\tgame.levelSetFriction (0)\t\n\tend\nend\n\nfunction T:checkAchievement ()\n\tif self.bonusCount >= self.numBonuses then\n\t\tgame.hudShowTimerLabel ("Collected!", "/gong.caf")\n\t\tif not game.openFeintIsAchievementUnlocked (OFA_TRAINING_JUMP_COMPLETED) then\n\t\t\tgame.openFeintUnlockAchievement (OFA_TRAINING_JUMP_COMPLETED)\n\t\tend\n\tend\nend\n'
