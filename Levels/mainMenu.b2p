S'function T:onEnter ()\n\tlocal b = game.isFreeVersion () and not game.areAllLevelsPurchased ()\t\n\tlevel:paddleWithName ("buyLevels2"):setEnabled (b)\n\n\tlocal messages\n\tmessages = {\n\t\t{kMessageTimeout, 5},\n\t\t{"newEscapeStart", "Tap here to start a full escape.", kEmoticonSurprised, 5, "", 6},\n\t\t{"newEscapeFrom", "Tap here to try an escape starting from any unlocked level.", kEmoticonAngel, 6, "", 7},\n\t\t{"inputMode", "Select you preferred input mode: slide or joystick.", kEmoticonSurprised, 5, "", 6},\n\t\t{"difficultyLevel", "Set your desired difficulty here!", kEmoticonSurprised, 5, "", 0},\n\t}\n\n\tself:startMessagesScript ("introMessages", messages)\n\nend\n\nfunction T:onExit ()\n\tself:stopMessagesScript ("introMessages")\nend\n\nfunction T:startLevel ()\n\tlocal levelIndex, levelTitle = game.levelCurrent ()\n\n\tif game.levelAvailable (levelIndex) then\n\t\tgame.startGameWithLevelIndex (levelIndex, kGMQuest, true)\n\telse\n\t\tgame.hudShowTimerLabel ("Locked", "/scoreBonusDown1.caf")\n\tend\nend\n\nfunction T:setLevel (levelIndex, levelTitle)\n\tlocal paddle = level:paddleWithName ("newEscapeFrom")\n\tlocal b = game.levelAvailable (levelIndex)\n\tlocal opacity = 255\n\t\n\tif not b then opacity = 128 end\n\n\tpaddle:setLabel (string.format ("Start from %s", levelTitle))\n\n\tlocal paddle = level:paddleWithName ("newEscapeFromThumbnailImage")\n\tpaddle:setTopImage (game.levelThumbnailImageName (levelIndex), 0.5, 0.5, 128, 86, 0.5, 0.5, opacity, 0)\nend\n\n'
