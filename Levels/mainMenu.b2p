S'function T:setup ()\n\tScreen.setup (self)\nend\n\nfunction T:onEnter ()\n\tlocal b = game.isFreeVersion () and not game.areAllLevelsPurchased ()\t\n\tlevel:paddleWithName ("buyLevels1"):setEnabled (b)\n\n\tlevel:paddleWithName ("rateNow"):setEnabled (game.shouldRate ())\n\tlevel:paddleWithName ("viewTrailer"):setEnabled (not game.wasBe2TrailerViewed ())\n\n\tlocal messages\n\tmessages = {\n\t\t{kMessageTimeout, 5},\n\t\t{mainHero, "Hello big fella!", kEmoticonHappy, 4, "", 5},\n\t\t{mainHero, "Let me introduce myself: I am Be2 the square.", kEmoticonHappy, 8, "", 11},\n\t\t{mainHero, "AND I WANT TO GET OUT OF HERE!", kEmoticonAngry, 8, "", 15},\n\t\t{"newEscape", "Tap here to start a new glorious quest toward freedom.", kEmoticonSurprised, 8, "", 10},\n\t\t{"trainingFields", "Tap here to train your moving skills with the STRANGE control modes.", kEmoticonAngel, 8, "", 10},\n\t}\n\n\tself:startMessagesScript ("introMessages", messages)\n\nend\n\nfunction T:onExit ()\n\tself:stopMessagesScript ("introMessages")\nend\n'
