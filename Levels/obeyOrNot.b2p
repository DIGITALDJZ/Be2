S'function T:setup ()\n\tScreen.setup (self)\n\n\tself.sr, self.sg, self.sb = self:color1 ()\n\n\tself.score = 2000\n\tself.screenTimeObey = 5\n\tself.screenTimeDisobey = 10\n\n\tself.numPaddles = 5\n\tself.minDistance = 100\nend               \n\nfunction T:onEnter ()\n\tScreen.onEnter (self)\n\n\tlevel:setMood ()\nend\n\nfunction T:repositionPaddles (first, last)\n\tlocal paddle\n\tlocal px, py, pw, ph\n\tlocal x, y = self:heroStartPosition ()\n\tlocal sx, sy = self:position ()\n\tx = x + sx\n\ty = y + sy\n\n\twhile first <= last do\n\t\tpaddle = level:paddleWithName (string.format ("s1p%d", first))\n\t\tpx, py = paddle:position ()\n\t\tif math.abs (px - x) < self.minDistance and math.abs (py - y) < self.minDistance then\n\t\t\tpaddle:setPosition (\n\t\t\t\tpx + (sign (px - x) * self.minDistance), \n\t\t\t\tpy + (sign (py - y) * self.minDistance)\n\t\t\t)\n\t\tend\n\t\tfirst = first + 1\n\tend\nend\n\nfunction T:resetScreen ()\n\tself:repositionPaddles (1, self.numPaddles)\n\tlevel:setPaddlesSpeed ("s1p%d", 1, self.numPaddles, 50, 90)\n\tlevel:enablePaddles ("s1p%d", 2, self.numPaddles, not level.isDisobeyMode)\n\n\tif level.isDisobeyMode then\n\t\tmainHero:setSpeed (0, 0)\n\telse\n\t\tmainHero:setSpeed (\n\t\t\trandomSign () * randomIntRange (70,120), \n\t\t\trandomSign () * randomIntRange (70,120)\n\t\t)\n\tend\nend\n\nfunction T:checkIfPassedOnTimeout ()\n\tlocal passed = false\n\n\tif level.isDisobeyMode then\n\t\tpassed = false\n\telse\n\t\tpassed = true\n\tend\n\n\treturn passed\nend\n\nfunction T:onPaddleHit (paddle, deadly)\n\tif deadly then\n\t\tif level.isDisobeyMode then\n\t\t\tlevel:onScreenPass ()\n\t\telse\n\t\t\tlevel:onScreenFail ()\n\t\tend\n\tend\nend\n'
