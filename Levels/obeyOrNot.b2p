S'\nfunction T:setup ()\n\tLevel.setup (self)\n\n\tgame.levelSetHUDTimeNotificationEnabled (false)\n\n\tself.neverFailed = true\n\n \tself.numScreensToExit = 30\n\tself.numScreensPassed = 0\n\n\tself.maxFailuresPerLifeDefault = 3\n\tself.maxFailuresPerLife = self.maxFailuresPerLifeDefault\n\tself.streaksPerMoreFailures = 5\n\n\tself.randomGap = 2\n\n\tself.minScreenSpeed = 1.0\n\tself.maxScreenSpeed = 3.0\n\tself.screenSpeed = 1.0\n\tself.screenSpeedInc = 0.1\n\tself.screenSpeedDec = 0.1\n\n\tself.isDisobeyMode = false\n\n\tself:resetOptions ()\n\tself:resetRandomScreens ()\nend\n\nfunction T:resetOptions ()\n\tself.streaks = 0\nend\n\nfunction T:enablePaddles (name, first, last, b)\n\twhile first <= last do\n\t\tself:paddleWithName (string.format (name, first)):setEnabled (b)\n\t\tfirst = first + 1\n\tend\nend\n\nfunction T:setPaddlesSpeed (name, first, last, mins, maxs)\n\tlocal paddle\n\twhile first <= last do\n\t\tpaddle = self:paddleWithName (string.format (name, first))\n\t\tpaddle:setSpeed (\n\t\t\t(randomSign () * randomIntRange (mins, maxs)) * self.screenSpeed, \n\t\t\t(randomSign () * randomIntRange (mins, maxs)) * self.screenSpeed\n\t\t)\n\t\tfirst = first + 1\n\tend\nend\n\nfunction T:setDisobey (disobey)\n\tself.isDisobeyMode = disobey\n\n\tlocal disobeyPaddle = self:paddleWithName (string.format ("%sDisobey", screen:name ()))\n\tdisobeyPaddle:setEnabled (disobey)\n\n\tlocal screenTime\n\n\tif disobey then\n\t\tscreen:setColor (180, 2, 3)\n\t\tscreenTime = screen.screenTimeDisobey\n\t\tplaySound ("disobey.caf")\n\telse\n\t\tscreen:setColor (screen.sr, screen.sg, screen.sb) \n\t\tscreenTime = screen.screenTimeObey\n\tend\n\n\tscreen:setAvailableTime (screenTime)\nend\n\nfunction T:onDie ()\n--\tself:setMood ()\nend\n\nfunction T:setMood ()\n\tlocal b = randomIntRange (0, self.randomGap) == 0\n\tself:setDisobey (b)\n\n\tscreen:resetScreen ()\n\n\tlocal msg = ""\n\tlocal emoticon = kEmoticonPlain\n\tlocal sound = ""\n\tlocal c = self.numScreensToExit - self.numScreensPassed\n\n\tif c <= 0 then\n\t\treturn\n\telseif c == 1 then\n\t\tmsg = "This is the last screen!!"\n\t\temoticon = kEmoticonHappy\n\telseif c < 5 then\n\t\tmsg = string.format ("Only %d screens left!", c)\n\t\temoticon = kEmoticonSurprised\n\telseif c < 10 then\n\t\tmsg = string.format ("Just %d screens!", c)\n\t\temoticon = kEmoticonPlain\n\telse\n\t\tmsg = string.format ("%d screens to go...", c)\n\t\temoticon = kEmoticonSad\n\tend\n\n\tmainHero:showMessageEmoticonDurationSound (msg, emoticon, 2, sound)\nend\n\nfunction T:onTimeLeftTimeout ()\n\tif screen:checkIfPassedOnTimeout () then\n\t\tself:onScreenPass ()\n\telse\n\t\tself:onScreenFail ()\n\tend\nend\n\nfunction T:resetRandomScreens ()\n\tself.randomScreens = {}\n\tfor i = 1, level:numScreens () do\n\t\tself.randomScreens[i] = string.format ("s%d", i)\n\tend\nend\n\nfunction T:getRandomScreen ()\n\tif #self.randomScreens == 0 then\n\t\tself:resetRandomScreens ()\n\tend\n\n\tlocal i = randomIntRange (1, #self.randomScreens)\n\tlocal sname = self.randomScreens[i]\n\n\twhile #self.randomScreens > 1 and screen and sname == screen:name () do\n\t\ti = randomIntRange (1, #self.randomScreens)\n\t\tsname = self.randomScreens[i]\n\tend\n\n\ttable.remove (self.randomScreens, i)\n\n\treturn sname\nend\n\nfunction T:tryNextScreen ()\n\tlocal sname = self:getRandomScreen ()\n\t\n--\tsname = "s7"\n\n\tlocal s = self:screenWithName (sname)\n\tlocal x, y = s:heroStartPosition ()\n\tmainHero:setSpeed (0, 0)\n\tgame.levelMoveMainHeroToScreenNameAtPosition (sname, x, y)\nend\n\nfunction T:onScreenPass ()\n\tgame.scoreAdd (screen.score)\n\tgame.hudShowTimerLabel ("Pass", "/elements/yeah.caf")\n\n\tself.numScreensPassed = self.numScreensPassed + 1\n\tif self.numScreensPassed == self.numScreensToExit then\n\t\tself:onLevelExit ()\n\tend\n\n\tself.streaks = self.streaks + 1\n\tif self.streaks >= self.streaksPerMoreFailures then\n\t\tself.maxFailuresPerLife = self.maxFailuresPerLife + 1\n\t\tself.streaks = 0\n\tend\n\n\tself.screenSpeed = clamp (self.screenSpeed + self.screenSpeedInc, self.minScreenSpeed, self.maxScreenSpeed)\n\n\tself:tryNextScreen ()\nend\n\nfunction T:onScreenFail ()\n\tself.neverFailed = false\n\n\tself.maxFailuresPerLife = self.maxFailuresPerLife - 1\n\n\tself:resetOptions ()\n\n\tif self.maxFailuresPerLife == 0 then\n\t\tself.maxFailuresPerLife = self.maxFailuresPerLifeDefault\n\t\tself.numScreensPassed = 0\n\t\tself.screenSpeed = self.minScreenSpeed\n\n\t\tgame.die (kDieTimeout)\n\telse\n\t\tgame.hudShowTimerLabel ("Fail", "/elements/oouh.caf")\n\t\tself.numScreensPassed = self.numScreensPassed - 1\n\t\tself.screenSpeed = clamp (self.screenSpeed - self.screenSpeedDec, self.minScreenSpeed, self.maxScreenSpeed)\n\n\t\tself:tryNextScreen ()\n\tend\nend\n\nfunction T:onLevelExit ()\n\tlocal seconds\n\tseconds = 5\n\n\tlevel:explorationPointsAdd ("end", 9)\n\n\tstopBackgroundMusic ()\n\n\tmainHero:setPaused (true)\n\tmainHero:showMessageEmoticonSound ("Yes!!! This is a really good TV show!", kEmoticonHappy, "levelExit.caf")\n\n\tgame.hudShowTimerLabel ("Exit!", "")\n\n\tlocal unlocked\n\tunlocked = false\n\tif not game.openFeintIsAchievementUnlocked (OFA_OBEY_OR_NOT_LEVEL_COMPLETED) then\n\t\tgame.openFeintUnlockAchievement (OFA_OBEY_OR_NOT_LEVEL_COMPLETED)\n\t\tunlocked = true\n\tend\n\n\tif self.neverFailed and not game.openFeintIsAchievementUnlocked (OFA_OBEY_OR_NOT_NEVER_FAILED) then\n\t\tgame.openFeintUnlockAchievement (OFA_OBEY_OR_NOT_NEVER_FAILED)\n\t\tunlocked = true\n\tend\n\n\tlocal acNum, acTot, acScore\n\tacNum = 0\n\tacTot = 0\n\tacScore = 0\n\n\tgame.levelSetCompleted (level.index, true)\n\tgame.levelSetAvailableWithName (level:nextLevelName (), true)\n\n\tlocal message, bonusScore\n\tlocal questElapsedTime, levelElapsedTime, levelTimeLeft = game.timeStats ()\n\tlocal score, scoreMultiplier, previousLevelScore, currentLevelScore = game.scoreStats ()\n\n\tif levelElapsedTime <= 300 and not game.openFeintIsAchievementUnlocked (OFA_OBEY_OR_NOT_ANARCHIST) then\n\t\tgame.openFeintUnlockAchievement (OFA_OBEY_OR_NOT_ANARCHIST)\n\t\tunlocked = true\n\tend\n\n\tif unlocked then seconds = seconds + 4 end\n\n\tlocal bonusScore = 0\n\n\tif self.neverFailed then\n\t\tbonusScore = bonusScore + 45000\n\tend\n\n\tif levelElapsedTime <= 300 then\n\t\tmessage = "Anarchy In Pongland!"\n\t\tbonusScore = bonusScore + 50000\n\telse\n\t\tmessage = "A Respectful Citizen"\n\tend\n\n\tgame.showLevelScorePanel (seconds, message, acNum, acTot, acScore, bonusScore, level:nextLevelName ())\nend\n'
